<nav class="navbarpages">
  <div class="navbarpages-logo">
    <img src=<%= image_path('Foxybeige.png') %> alt="Logo Beige">
  </div>
  <div class="navbarpages-profile">
    <% if user_signed_in? %>
      <a href=<%= user_path(current_user) %> >
        <%= cl_image_tag current_user.photo.key if current_user.photo.attached? %>
      </a>
    <% else %>
      <%= link_to "Login", new_user_session_path %>
    <% end %>
  </div>
</nav>
